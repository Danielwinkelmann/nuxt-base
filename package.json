{
  "name": "@lenne.tech/nuxt-base",
  "version": "1.1.4",
  "description": "Nuxt base module of @lenne.tech",
  "repository": "https://github.com/lenneTech/nuxt-base",
  "license": "MIT",
  "type": "module",
  "author": "lenne.Tech <info@lenne.tech> (https://lenne.tech)",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run dev:build && nuxt-module-build && npm run copy",
    "dev": "nuxi dev playground",
    "reinit": "rm -rf node_modules && rm -rf package-lock.json && npm cache clean --force && npm i",
    "copy": "cp package.json ./dist/package.json && cp README.md ./dist/README.md",
    "dev:build": "nuxi build playground",
    "build:ci": "nuxt build && npm run copy",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "standard-version && git push --follow-tags origin main",
    "release:minor": "standard-version --release-as minor && git push --follow-tags origin main",
    "release:major": "standard-version --release-as major && git push --follow-tags origin main",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@graphql-codegen/typescript": "3.0.2",
    "@nuxt/kit": "3.3.1",
    "@nuxtjs/apollo": "5.0.0-alpha.6",
    "@pinia/nuxt": "0.4.7",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "js-sha256": "0.9.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.2",
    "@graphql-codegen/client-preset": "2.1.1",
    "@nuxt/eslint-config": "0.1.1",
    "@nuxt/module-builder": "0.2.1",
    "@nuxt/schema": "3.3.1",
    "@nuxt/test-utils": "3.3.1",
    "eslint": "8.36.0",
    "nuxt": "3.3.1",
    "standard-version": "9.5.0",
    "typescript": "5.0.2",
    "vitest": "0.29.3"
  }
}
